@model IEnumerable<CreateCourseViewModel>
<div class="row">
    <table class="table table-responsive table-striped">
        <thead>
            <tr>
                <th>Course Name</th>
                <th>Starting Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name, </td>
                    <td>@item.StartDate</td>
                </tr>


            }
        </tbody>
    </table>
</div>